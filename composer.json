{
    "name": "adangel/chunkphp",
    "description": "Simple hosting of temporary files",
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "ext-zip": "*",
        "ext-phar": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "psr-4": {
            "com\\github\\adangel\\chunkphp\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "com\\github\\adangel\\chunkphp\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit tests"
    }
}
